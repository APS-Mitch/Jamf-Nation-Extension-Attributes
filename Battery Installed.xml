<?xml version="1.0" encoding="UTF-8"?><extensionAttribute>	<displayName>Battery Installed</displayName>	<displayInCategory>System Information</displayInCategory>	<dataType>string</dataType>	<description>This attribute returns whether or not a battery is currently installed in a system that supports batteries.</description>	<scriptContentsMac>#!/bin/shecho "&lt;result&gt;$(ioreg -r -c "AppleSmartBattery" | grep "BatteryInstalled" | awk '{print $3}' | sed s/\"//g)&lt;/result&gt;"	</scriptContentsMac></extensionAttribute>