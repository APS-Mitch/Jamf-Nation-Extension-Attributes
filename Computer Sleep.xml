<?xml version="1.0" encoding="UTF-8"?><extensionAttribute>	<displayName>Computer Sleep</displayName>	<displayInCategory>Power Management</displayInCategory>	<description>This attribute displays the number of minutes before the computer sleeps on the active power management profile.</description>	<dataType>integer</dataType>	<scriptContentsMac>#!/bin/shecho "&lt;result&gt;`/usr/bin/pmset -g 2>&amp;1 | grep -w sleep | awk '{print $2}'`&lt;/result&gt;"	</scriptContentsMac></extensionAttribute>