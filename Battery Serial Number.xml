<?xml version="1.0" encoding="UTF-8"?><extensionAttribute>	<displayName>Battery Serial Number</displayName>	<displayInCategory>System Information</displayInCategory>	<dataType>string</dataType>	<description>This attribute returns serial number of the battery, if installed.</description>	<scriptContentsMac>#!/bin/shecho "&lt;result&gt;$(ioreg -r -c "AppleSmartBattery" | grep "BatterySerialNumber" | awk '{print $3}' | sed s/\"//g)&lt;/result&gt;"	</scriptContentsMac></extensionAttribute>