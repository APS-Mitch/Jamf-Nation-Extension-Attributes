<?xml version="1.0" encoding="UTF-8"?><extensionAttribute>	<displayName>Battery Charging</displayName>	<displayInCategory>System Information</displayInCategory>	<dataType>string</dataType>	<description>This attribute returns whether or not the battery is charging, if installed.</description>	<scriptContentsMac>#!/bin/shecho "&lt;result&gt;$(ioreg -r -c "AppleSmartBattery" | grep -w "IsCharging" | awk '{print $3}' | sed s/\"//g)&lt;/result&gt;"	</scriptContentsMac></extensionAttribute>